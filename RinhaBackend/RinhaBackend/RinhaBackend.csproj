    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>201541a1-d3a9-44fe-b812-3383d2987093</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
